{
    "name": "plesk/api-php-lib",
    "description": "PHP object-oriented library for Plesk XML-RPC API",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Alexei Yuzhakov",
            "email": "sibprogrammer@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^5.6|^7.1|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "PleskX\\": "src/PleskX/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
